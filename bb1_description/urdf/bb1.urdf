<?xml version="1.0"?>
<robot name="burt">

	<material name="gray">
		<color rgba="0.75 0.75 0.75 2"/>
	</material>

	<material name="dark_gray">
		<color rgba="0.25 0.25 0.25 2"/>
	</material>

	<material name="wood">
		<color rgba="0.76 0.6 0.42 2"/>
	</material>

	<link name="base_link">
		<visual>
			<geometry>
				<box size="0.30 0.25 0.058"/>
			</geometry>
			<material name="wood"/>
		</visual>
		<collision>
			<geometry>
				<box size="0.30 0.25 0.058"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1"/>
			<inertia ixx="0.125" ixy="0" ixz="0" iyy="0.166" iyz="0" izz="0.266"/>
		</inertial>
	</link>

	<link name="front_right_wheel">
		<visual>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 -0.03 0"/>
			<material name="dark_gray"/>
		</visual>
		<collision>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 -0.03 0"/>
		</collision>
		<inertial>
			<mass value="3"/>
			<inertia ixx="0.006" ixy="0" ixz="0" iyy="0.006" iyz="0" izz="0.011"/>
		</inertial>
	</link>
	<joint name="front_right_wheel_joint" type="continuous">
		<parent link="base_link"/>
		<child link="front_right_wheel"/>
		<origin xyz="0.09 -0.135 -0.035"/>
		<axis rpy="0 0 0" xyz="0 1 0"/>
	</joint>

	<link name="back_right_wheel">
		<visual>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 -0.03 0"/>
			<material name="dark_gray"/>
		</visual>
		<collision>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 -0.03 0"/>
		</collision>
		<inertial>
			<mass value="3"/>
			<inertia ixx="0.006" ixy="0" ixz="0" iyy="0.006" iyz="0" izz="0.011"/>
		</inertial>
	</link>
	<joint name="back_right_wheel_joint" type="continuous">
		<parent link="base_link"/>
		<child link="back_right_wheel"/>
		<origin xyz="-0.09 -0.135 -0.035"/>
		<axis rpy="0 0 0" xyz="0 1 0"/>
	</joint>

	<link name="front_left_wheel">
		<visual>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 0.03 0"/>
			<material name="dark_gray"/>
		</visual>
		<collision>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 0.03 0"/>
		</collision>
		<inertial>
			<mass value="3"/>
			<inertia ixx="0.006" ixy="0" ixz="0" iyy="0.006" iyz="0" izz="0.011"/>
		</inertial>
	</link>
	<joint name="front_left_wheel_joint" type="continuous">
		<parent link="base_link"/>
		<child link="front_left_wheel"/>
		<origin xyz="0.09 0.135 -0.035"/>
		<axis rpy="0 0 0" xyz="0 1 0"/>
	</joint>

	<link name="back_left_wheel">
		<visual>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 0.03 0"/>
			<material name="dark_gray"/>
		</visual>
		<collision>
			<geometry>
				<cylinder length="0.045" radius="0.085"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 0.03 0"/>
		</collision>
		<inertial>
			<mass value="3"/>
			<inertia ixx="0.006" ixy="0" ixz="0" iyy="0.006" iyz="0" izz="0.011"/>
		</inertial>
	</link>
	<joint name="back_left_wheel_joint" type="continuous">
		<parent link="base_link"/>
		<child link="back_left_wheel"/>
		<origin xyz="-0.09 0.135 -0.035"/>
		<axis rpy="0 0 0" xyz="0 1 0"/>
	</joint>

<!-- This block provides the simulator (Gazebo) with information on a few additional
physical properties. See http://gazebosim.org/tutorials/?tut=ros_urdf for more-->
<gazebo reference="front_right_wheel">
  <mu1 value="200.0"/>
  <mu2 value="100.0"/>
  <kp value="10000000.0" />
  <kd value="1.0" />
  <material>Gazebo/Grey</material>
</gazebo>

<gazebo reference="back_right_wheel">
  <mu1 value="200.0"/>
  <mu2 value="100.0"/>
  <kp value="10000000.0" />
  <kd value="1.0" />
  <material>Gazebo/Grey</material>
</gazebo>

<gazebo reference="front_left_wheel">
  <mu1 value="200.0"/>
  <mu2 value="100.0"/>
  <kp value="10000000.0" />
  <kd value="1.0" />
  <material>Gazebo/Grey</material>
</gazebo>

<gazebo reference="back_left_wheel">
  <mu1 value="200.0"/>
  <mu2 value="100.0"/>
  <kp value="10000000.0" />
  <kd value="1.0" />
  <material>Gazebo/Grey</material>
</gazebo>


<!-- This block connects the wheel joint to an actuator (motor), which informs both
simulation and visualization of the robot -->
<transmission name="base_to_right_front_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <actuator name="base_to_right_front_wheel_motor">
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
  <joint name="front_right_wheel_joint">
    <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>
</transmission>

<transmission name="base_to_right_back_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <actuator name="base_to_right_back_wheel_motor">
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
  <joint name="back_right_wheel_joint">
    <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>
</transmission>

<transmission name="base_to_front_left_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <actuator name="base_to_front_left_wheel_motor">
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
  <joint name="front_left_wheel_joint">
    <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>
</transmission>

<transmission name="base_to_back_left_wheel_trans">
  <type>transmission_interface/SimpleTransmission</type>
  <actuator name="base_to_back_left_wheel_motor">
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
  <joint name="back_left_wheel_joint">
    <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </joint>
</transmission>

<gazebo>
	<plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
	<robotNamespace>/</robotNamespace>
	</plugin>
</gazebo>

</robot>