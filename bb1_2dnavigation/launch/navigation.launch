<?xml version="1.0"?>
<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find bb1_2dnavigation)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find bb1_2dnavigation)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find bb1_2dnavigation)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find bb1_2dnavigation)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find bb1_2dnavigation)/config/base_local_planner_params.yaml" command="load" />

    <remap from="cmd_vel_mux/input/navi" to="cmd_vel"/>

  </node>
</launch>
